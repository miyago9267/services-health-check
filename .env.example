CHECK_NAME=example-http
CHECK_URL=https://example.com
CHECK_INTERVAL=30s
CHECK_TIMEOUT=5s
CHECK_SCHEDULE=*/5 * * * *
CHECK_SKIP_VERIFY=false
CHECK_DOMAIN=example.com
CHECK_TOKEN=your-cloudflare-token

